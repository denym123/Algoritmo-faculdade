programa {
  inteiro n1
  inteiro n2
  inteiro resultado
  funcao inicio() {
    escreva("Seja bem-vindo ao Aplicativo de soma", "\n")
    escreva("Digite o primeiro numero: ")
    leia(n1)
    escreva("Digite o segundo numero: ")
    leia(n2)
    resultado = (n1+n2)
    escreva("O resultado da soma de ", n1, " + ", n2, " é: ", resultado)
  }
}
